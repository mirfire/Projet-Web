{% extends 'base.html.twig' %}

{% block body %}
<div class="container">
    <h1 class="text-center">Modifier un projet</h1>
    <form action="" method="post">
        <div>
            <label>Nom du projet</label>
            <input type="Text" id="nameproject"/>
        </div>
        <div>
            <label>Description</label>
            <input type="Text" id="description"/>
        </div>
        <div>
            <label>Illustration (Photos)</label>
            <input type="Image" id="Illustration"/>
        </div>
        <div>
            <label>Liens vers la réalisation</label>
            <input type="url" id="hyperlien"/>
        </div>
        <div>
            <label>Téléchargement du projet(si disponible)</label>
            <input type="file" id="download"/>
        </div>
        <div class="button">
            <button type="submit">Lancer la modification</button>
        </div>
    </form>
</div>
{% endblock %}
